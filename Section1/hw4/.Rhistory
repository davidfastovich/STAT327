x <- seq(min(x.coord), max(x.coord), length.out = 100)
y.coord <- (2+(3*x.coord) + e.i)
fit.sim <- lm(y.coord ~ x.coord)
plot(x = x.coord, y = y.coord)
abline(fit.sim)
abline(2, 3, lty = 2, col = "red")
preds <- predict(fit.sim, interval = "confidence")
lines(sort(x.coord, decreasing = TRUE), sort(preds[,2], decreasing = TRUE), lty = 2, col = "blue")
lines(sort(x.coord, decreasing = TRUE), sort(preds[,3], decreasing = TRUE), lty = 2, col = "blue")
summary(fit.sim)
